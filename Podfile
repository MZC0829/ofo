use_frameworks!
target 'ofo' do
    pod 'AMap3DMap' #3D地图,与2D地图不能同时使用
    pod 'AMapLocation' #定位功能
    pod 'AMapSearch' #搜索功能
    pod 'AMapNavi'  #导航功能,已包含3D地图，无需单独引入3D地图
    pod 'SWRevealViewController' #侧边栏
    pod 'FTIndicator'
    pod 'swiftScan', '~> 1.0.9'
    pod 'APNumberPad'
    pod 'SwiftyTimer'
    pod 'SwiftySound'
    pod 'AVOSCloud'  # 数据存储、短信、云引擎调用等基础服务模块(LeanCloud oc版本)
    pod 'LeanCloud'  #LeanCloud swift版本    
end
